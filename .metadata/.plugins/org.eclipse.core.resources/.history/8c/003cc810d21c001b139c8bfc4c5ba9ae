import java.util.*;

public class Example{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("(정수-1이 입력될 때까지)>>");
		int num = 0;
		int bigNum = 0;
		while(num != -1) {
			num = scanner.nextInt();
			if(num > 0) {
				System.out.print(num + " ");
				if(bigNum < num) {
					bigNum = num;
				}
			}
		}
		System.out.println();
		System.out.println("가장 큰 수는" + bigNum);
		scanner.close();
	}
}