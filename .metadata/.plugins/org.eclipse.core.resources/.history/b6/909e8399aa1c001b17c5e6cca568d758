import java.util.*;
public class PracticeClass {
	public static void main(String[] args) {
		ArrayList<String> a = new ArrayList<>();
		a.add("java");
		a.add(0,"C++");
		Iterator<String> it = a.iterator();
		int i = 0;
		while(it.hasNext()) {
			String n = it.next();
			System.out.print(n);
			System.out.println(i);
			i++;
		}
		System.out.println(a.size());
		a.remove(a.size()-1);
		System.out.println(a.size());
		
	}
}
